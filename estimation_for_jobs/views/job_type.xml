<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="action_job_types" model="ir.actions.act_window">
            <field name="name">Job Types</field>
            <field name="res_model">job.type</field>
            <field name="view_mode">tree</field>
        </record>


        <record id="action_job_types_tree" model="ir.ui.view">
            <field name="name">action.job.types.tree</field>
            <field name="model">job.type</field>
            <field name="arch" type="xml">
                <tree string="Tag" editable="top" >
                    <field name="job_type"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>